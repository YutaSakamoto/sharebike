$('#photos').html("<%= j render 'photos_list' %>")
<% if @motorbike.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@motorbike.active && !@motorbike.price.blank? && !@motorbike.listing_name.blank? && !@motorbike.photos.blank? && !@motorbike.address.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>
